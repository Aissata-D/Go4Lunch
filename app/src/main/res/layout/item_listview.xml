<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="111dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/resto_like_number_item_listview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/resto_imageview_item_listview"
            app:drawableStartCompat="@drawable/ic_baseline_star_24_yellow" />

        <TextView
            android:id="@+id/resto_opening_hour_item_listview"
            android:layout_width="220dp"
            android:layout_height="wrap_content"
            android:layout_margin="8dp"
            android:text="Opening hour"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/resto_like_number_item_listview"
            app:layout_constraintHorizontal_bias="0.085"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/resto_workmater_number_item_listview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="(0)"
            android:layout_margin="8dp"
            app:drawableStartCompat="@drawable/ic_baseline_person_outline_24"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/resto_imageview_item_listview"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/resto_type_adresse_item_listview"
            android:layout_width="220dp"
            android:layout_height="wrap_content"
            android:layout_margin="8dp"
            android:text="Type - adresse"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/resto_workmater_number_item_listview"
            app:layout_constraintHorizontal_bias="0.074"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/resto_imageview_item_listview"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:layout_margin="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/resto_name_item_listview"
            android:layout_width="220dp"
            android:layout_height="wrap_content"
            android:layout_margin="8dp"
            android:text="NAME RESTO"
            android:textStyle="bold"
            app:layout_constraintEnd_toStartOf="@+id/distance_item_listview"
            app:layout_constraintHorizontal_bias="0.043"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/distance_item_listview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="8dp"
            android:text="Xm"
            app:layout_constraintEnd_toStartOf="@+id/resto_imageview_item_listview"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>